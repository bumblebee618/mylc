import java.math.BigDecimal;
import java.util.*;

import javax.management.relation.Role;
import javax.xml.crypto.dsig.keyinfo.KeyInfo;







public class Q000_A_Contest 
{	  
	public String findDifferentBinaryString(String[] nums) {
        Set<Integer> set = new HashSet<>();
        
        for (String num : nums) {
        	int sum = 0;
        	
            for (int i = 0; i < num.length(); i++) {
            	if (num.charAt(i) == '1') {
            		sum += (1 << i);
            	}
            }
            
            set.add(sum);
        }
        
        int n = nums.length;
        int end = (1 << n) - 1; 
        
        int target = -1;
        
        for (int i = 0; i <= end; i++) {
        	if (!set.contains(i)) {
        		target = i;
        		break;
        	}
        }
        
      System.out.println("***" + target);

        StringBuilder builder = new StringBuilder();
        
        while (target > 0) {
        	builder.insert(0, target % 2);
        	target >>= 1;
        }
        
        while (builder.length() < n) {
        	builder.insert(0, '0');
        }
        
        return builder.toString();
    }
    
    
    
	
	
    public static void main(String[] args)
    {
    	Q000_A_Contest test = new Q000_A_Contest();
    	
    	/****************************************************/
    	
    	String[] nums1 = {"01","10"};
    	String[] nums2 = {"00","01"};
    	String[] nums3 = {"111","011","001"};
    	
    	System.out.println(test.findDifferentBinaryString(nums1));
    	System.out.println(test.findDifferentBinaryString(nums2));
    	System.out.println(test.findDifferentBinaryString(nums3));
    	
    }
}

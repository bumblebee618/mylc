public class ReserveResponse 
{
	private String orderId;
	private Status status;
	
	public ReserveResponse(String orderId, Status status)
	{
		this.orderId = orderId;
		this.status = status;
	}

	public UserOrder getOrderId() 
	{
		return orderId;
	}

	public Status getStatus() 
	{
		return status;
	}
}

enum Status
{
	SUCCEED(), FAIL();
}

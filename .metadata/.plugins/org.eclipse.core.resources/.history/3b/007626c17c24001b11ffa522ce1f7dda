public class Q081_Search_in_Rotated_Sorted_Array_II {
	/****************************************************/
	// by other using binary search
	// 分类讨论
	public boolean search(int[] nums, int target) {
		if (nums == null || nums.length == 0)
        {
            return false;
        }
        
        int left = 0, right = nums.length-1;
        
        while (left+1 < right)
        {
            int mid = left + (right - left) / 2;
            
            if (nums[mid] == target || nums[left] == target || nums[right] == target)
            {
                return true;
            }
            
            // “||”后面的判断是为了防止只有两个元素时
            if (nums[mid] > nums[left] || nums[mid] > nums[right])
            {
                if (target > nums[left] && target < nums[mid])
                {
                    right = mid;
                }
                else
                {
                    left = mid;   
                }
            }
            else if (nums[mid] < nums[left] || nums[mid] < nums[right])
            {
                if (target > nums[mid] && target < nums[right])
                {
                    left = mid;
                }
                else
                {
                    right = mid;   
                }
            }
            else
            {
                right--;
            }
        }
        
        return (nums[left] == target || nums[right] == target) ? true : false;
		
		
		
		int start = 0, end = nums.length - 1, mid = -1;
        while(start <= end) {
            mid = (start + end) / 2;
            if (nums[mid] == target) {
                return true;
            }
            //If we know for sure right side is sorted or left side is unsorted
            if (nums[mid] < nums[end] || nums[mid] < nums[start]) {  // “||”后面的判断是为了防止只有两个元素时
                if (target > nums[mid] && target <= nums[end]) {
                    start = mid + 1;
                } else {
                    end = mid - 1;
                }
            //If we know for sure left side is sorted or right side is unsorted
            } else if (nums[mid] > nums[start] || nums[mid] > nums[end]) {
                if (target < nums[mid] && target >= nums[start]) {   // “||”后面的判断是为了防止只有两个元素时
                    end = mid - 1;
                } else {
                    start = mid + 1;
                }
            // If we get here, that means nums[start] == nums[mid] == nums[end], then shifting out
            // any of the two sides won't change the result but can help remove duplicate from
            // consideration, here we just use end-- but left++ works too
            } else {
                end--;
            }
        }

        return false;
	}
	


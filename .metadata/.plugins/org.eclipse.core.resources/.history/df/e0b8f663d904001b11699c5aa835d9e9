
public class VehicleFactory {
	public static Vehicle createVehicle(String type, int plateNum)
	{
		switch (type)
		{
			case "Motorcycle": return new Motorcycle(plateNum); break;
			case "Car": return new Car(plateNum); break;
			case "Bus": return new Bus(plateNum); break;
			default: return null;
		}
	}
}

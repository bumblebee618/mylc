public abstract class Vehicle {
	protected int plateNum;
	protected int size;
	
	public Vehicle(int plateNum)
	{
		this.plateNum = plateNum;
		setSize(0);
	}

	public int getPlateNum() {
		return plateNum;
	}

	public int getSize() {
		return size;
	}

	public void setSize(int size) {
		this.size = size;
	}
}

import java.util.List;

public class CasherFactory 
{
	public static double getBalance(CasherType type, List<Pizza> pizzas)
	{
		switch (type)
		{
			case DISCOUNT: return new CasherDiscount(pizzas).getFinalPrice();
			case NORMAL: 
			default: return new CasherNormal(pizzas).getFinalPrice();
		}
	}
}


public class VehicleFactory {
	public static Vehicle createVehicle(String type, int plateNum)
	{
		switch (type)
		{
			case "Vehicle_Motorcycle": return new Vehicle_Motorcycle(plateNum); 
			case "Vehicle_Car": return new Vehicle_Car(plateNum); 
			case "Vehicle_Bus": return new Vehicle_Bus(plateNum); 
			default: return null;
		}
	}
}

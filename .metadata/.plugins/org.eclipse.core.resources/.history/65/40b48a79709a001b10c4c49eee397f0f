import java.util.*;




public class Q000_A_Contest 
{
	public int arraySign(int[] nums) 
    {
        if (nums == null || nums.length == 0)
        {
            return 0;
        }
        
        int result = 1;
        
        for (int num : nums)
        {
            if (num < 0)
            {
                result *= -1;
            }
            else if (num == 0)
            {
                return 0;
            }
        }
        
        return result;
    }



    
    public static void main(String[] args)
    {
    	Q000_A_Contest test = new Q000_A_Contest();
    	
    	/****************************************************/
    	
    	int[] nums1 = {-1,-2,-3,-4,3,2,1};
    	int[] nums2 = {1,5,0,2,-3};
    	int[] nums3 = {-1,1,-1,1,-1};
    	System.out.println(test.arraySign(nums1));
    	System.out.println(test.arraySign(nums2));
    	System.out.println(test.arraySign(nums3));
    	
    	
    	
    }
}
